{
    "name": "My Dev Container",
    "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:14",
    "extensions": [
        "GitHub.copilot",
        "GitHub.copilot-labs",
        "GitHub.copilot-chat"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
      "customizations": {
        "vscode": {
            "extensions": [
                "GitHub.copilot"
            ]
        },
        "codespaces": {
            "openFiles": [
                "codespace.md"
            ]
        }
    },
    "forwardPorts": [80, 443]
}
